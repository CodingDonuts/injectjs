{
  "name": "node-injectjs",
  "version": "1.0.3",
  "description": "A NodeJS Dependency Management Framework",
  "main": "src/core/Core.js",
  "scripts": {
    "testrunner": "mocha -w --recursive test*/*",
    "test": "mocha test*/**/*spec.js",
