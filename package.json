{
  "name": "node-injectjs",
  "version": "1.0.4",
  "description": "A NodeJS Dependency Management Framework",
  "main": "src/core/Core.js",
  "scripts": {
    "preinstall": "rm -f .git/hooks/pre-commit.sample && ln -s -f ../../githooks/pre-commit .git/hooks/pre-commit",
    "testrunner": "mocha -w --recursive test*/*",
    "test": "mocha test*/*",
    "jshint": "./githooks/pre-commit",
    "coverage": "istanbul cover _mocha -- --recursive -R node_modules/mocha-spec-cov-alt test/*"
  },
  "bin": {
    "injectjs": "bin/client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankrafael/injectjs.git"
  },
  "author": "Rafael Frank",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frankrafael/injectjs/issues"
  },
  "homepage": "https://github.com/frankrafael/injectjs#readme",
  "devDependencies": {
    "bithound": "^1.2.0",
    "body-parser": "^1.13.1",
    "chai": "^3.2.0",
    "chai-spies": "^0.7.0",
    "connect-mongo": "^0.8.1",
    "express": "^4.12.3",
    "express-session": "^1.11.3",
    "grunt": "^0.4.5",
    "grunt-exec": "^0.4.6",
    "gruntify-eslint": "^2.0.0",
    "istanbul": "^0.4.3",
    "jshint": "^2.9.2",
    "mocha": "^2.2.5",
    "mocha-spec-cov-alt": "^1.1.1",
    "mongodb": "^2.0.33",
    "node-gcm": "^0.9.15",
    "socket.io": "^1.3.5"
  },
  "files": [
    "bin",
    "src"
  ]
}
