  },
  "homepage": "https://github.com/frankrafael/injectjs#readme",
  "devDependencies": {
    "body-parser": "^1.13.1",
    "chai": "^3.2.0",
    "chai-spies": "^0.7.0",
    "connect-mongo": "^0.8.1",
    "coveralls": "^2.11.4",
    "express": "^4.12.3",
    "express-session": "^1.11.3",
    "grunt": "^0.4.5",
    "grunt-exec": "^0.4.6",
    "gruntify-eslint": "^2.0.0",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
