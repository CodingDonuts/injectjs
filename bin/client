#!/usr/bin/env node
module.exports = function(){
    'use strict';

    process.title = 'injectjs';

    var aArguments = process.argv.splice(2);
    var sCurrentPath = process.cwd();

    if (aArguments[0] === 'init'){
        var sPath = sCurrentPath; //TODO: Make configurable - Ask user
        return require('./tools/generator.js')(sPath);
    }

}(arguments);
